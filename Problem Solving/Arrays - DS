// https://www.hackerrank.com/challenges/arrays-ds/problem

static int[] reverseArray(int[] a) {
        int start = 0;
        int end = a.length-1;
        int temp;   
        while (start < end) 
        { 
            temp = a[start];  
            a[start] = a[end]; 
            a[end] = temp; 
            start++; 
            end--; 
        }  
        return a;
    }
